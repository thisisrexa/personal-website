---
import { Client, Databases, Query } from 'appwrite';
import Layout from '@/layouts/Layout.astro';
import { Content as Biography } from '@/content/biography.md';

const client = new Client();

client
  .setEndpoint('https://cloud.appwrite.io/v1')
  .setProject('64a6bc12320815d7767a');

const databases = new Databases(client);

const projects = await databases
  .listDocuments('64a6bd06620104b31e88', '64a6bd10c9a678361da8', [
    Query.orderDesc('year'),
    Query.orderAsc('title'),
    Query.equal('draft', false),
  ])
  .then((res) => res.documents);

const templates = await databases
  .listDocuments('64a6bd06620104b31e88', '64a6d54febf5f250851f', [
    Query.orderDesc('pubDate'),
    Query.orderAsc('title'),
    Query.equal('draft', false),
  ])
  .then((res) => res.documents);
